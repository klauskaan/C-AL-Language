    PROCEDURE ProcessMultilingüalData@1(Parám1@1000 : Text[100];Paramétér2@1001 : Integer);
    VAR
      TëmpVär@2000 : Text;
      Rësült@2001 : Boolean;
      Cöüntér@2002 : Integer;
    BEGIN
      // Demonstrate Unicode in identifiers and processing
      TëmpVär := STRSUBSTNO(TXT026,Parám1,Paramétér2);
      MESSAGE(TXT001);

      FOR Cöüntér := 1 TO Paramétér2 DO BEGIN
        Rësült := ValidateÜnicodeString(TëmpVär);
        IF Rësült THEN
          MESSAGE(TXT010)
        ELSE
          ERROR(TXT003);
      END;
    END;

    PROCEDURE ValidateÜnicodeString@2(InputStríng@1000 : Text) : Boolean;
    VAR
      Längth@2000 : Integer;
      Ïndëx@2001 : Integer;
    BEGIN
      Längth := STRLEN(InputStríng);
      MESSAGE(TXT021,Längth);

      IF Längth = 0 THEN
        ERROR(TXT112);

      EXIT(TRUE);
    END;

