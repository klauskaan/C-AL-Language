                        IF ProcessACMETypeA(ACMERecord."No.", ProcessingLevel) THEN BEGIN
                          UpdateFooBarIterator(FooBarIterator, ProcessingLevel);
                          DecrementTestCoLimit(TestCoLimit);
                        END ELSE BEGIN
                          LogACMEProcessingError(ACMERecord."No.", ProcessingLevel);
                          BreakFooBarLoop(FooBarIterator);
                        END;

                        ProcessingLevel := ProcessingLevel - 1;
                      END;
                    END ELSE BEGIN
                      ApplyTestCoMinimumProcessing(TestCoLimit);
                    END;
                  END;
                'TYPE-B':
                  BEGIN
                    ProcessACMETypeB(ACMERecord."No.", FooBarIterator);
                  END;
