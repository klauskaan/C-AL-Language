                  END;
              END;
            END ELSE BEGIN
              SkipFooBarIteration(FooBarIterator);
            END;

            FooBarIterator := FooBarIterator + 1;
          END;
        END ELSE BEGIN
          LogACMENoRecordsFound(FooBarStatus);
        END;

        WhileCounter := WhileCounter + 1;
      END;
    END;

    PROCEDURE TestRepeatUntilComplexity@4();
