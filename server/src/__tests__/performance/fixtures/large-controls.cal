    VAR
      ShowNotifications@1000 : Boolean;

    LOCAL PROCEDURE SellToCustomerNoOnAfterValidate@1();
    BEGIN
      CurrPage.UPDATE;
    END;

    LOCAL PROCEDURE ValidateRequestedDeliveryDate@2();
    BEGIN
      IF "Requested Delivery Date" < "Order Date" THEN
        ERROR('Requested delivery date cannot be before order date');
    END;

    LOCAL PROCEDURE CalculateTotals@3();
    BEGIN
      CALCFIELDS("Amount Including VAT","Total VAT Amount");
    END;

    LOCAL PROCEDURE ValidatePaymentMethod@4();
    BEGIN
      IF "Payment Method Code" = '' THEN
        MESSAGE('Payment method should be specified');
    END;

    LOCAL PROCEDURE PostDocumentAndCreateNew@5();
    VAR
      SalesHeader@1000 : Record 36;
    BEGIN
      CODEUNIT.RUN(CODEUNIT::"Sales-Post (Yes/No)",Rec);
      SalesHeader.INIT;
      SalesHeader."Document Type" := "Document Type"::Order;
      SalesHeader.INSERT(TRUE);
      PAGE.RUN(PAGE::"Sales Order",SalesHeader);
    END;

    LOCAL PROCEDURE ShowApprovals@6();
    VAR
      ApprovalEntry@1000 : Record 454;
    BEGIN
      ApprovalEntry.SETRANGE("Table ID",DATABASE::"Sales Header");
      ApprovalEntry.SETRANGE("Document Type","Document Type");
      ApprovalEntry.SETRANGE("Document No.","No.");
      PAGE.RUN(PAGE::"Approval Entries",ApprovalEntry);
    END;

    LOCAL PROCEDURE ShowComments@7();
    VAR
      SalesCommentLine@1000 : Record 44;
    BEGIN
      SalesCommentLine.SETRANGE("Document Type","Document Type");
      SalesCommentLine.SETRANGE("No.","No.");
      PAGE.RUN(PAGE::"Sales Comment Sheet",SalesCommentLine);
    END;

    BEGIN
    END.
  }
